class Person:
   def _init_(self, person_id, name, age, address, phone):
       self.person_id = person_id
       self.name = name
       self.age = age
       self.address = address
       self.phone = phone
 
   def display_info(self):
       print(f"Person ID: {self.person_id}")
       print(f"Name: {self.name}")
       print(f"Age: {self.age}")
       print(f"Address: {self.address}")
       print(f"Phone: {self.phone}")
 
 
class PersonManager:
   def _init_(self):
       self.persons = []
 
   def add_person(self, person):
       self.persons.append(person)
 
   def remove_person(self, person_id):
       for person in self.persons:
           if person.person_id == person_id:
               self.persons.remove(person)
               return
       print("Person not found.")
 
   def search_person(self, person_id):
       for person in self.persons:
           if person.person_id == person_id:
               return person
       return None
 
   def display_all_persons(self):
       if not self.persons:
           print("No persons found.")
           return
       for person in self.persons:
           person.display_info()
 
 
# Usage example
person1 = Person("P001", "John Doe", 30, "123 Main St", "123-456-7890")
person2 = Person("P002", "Jane Smith", 25, "456 Elm St", "987-654-3210")
person3 = Person("P003", "Alice Johnson", 35, "789 Oak St", "555-123-4567")
 
person_manager = PersonManager()
person_manager.add_person(person1)
person_manager.add_person(person2)
person_manager.add_person(person3)
 
person_manager.display_all_persons()
 
search_person = person_manager.search_person("P002")
if search_person:
   print("Person found:")
   search_person.display_info()
else:
   print("Person not found.")
 
person_manager.remove_person("P003")
 
person_manager.display_all_persons()
